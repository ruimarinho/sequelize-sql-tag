{
  "name": "sequelize-sql-tag",
  "version": "1.0.0",
  "description": "A sequelize plugin for sql-tag",
  "license": "MIT",
  "author": {
    "name": "Rui Marinho",
    "email": "rui.marinho@seegno.com",
    "url": "https://github.com/ruimarinho"
  },
  "homepage": "https://github.com/seegno/sequelize-sql-tag",
  "bugs": "https://github.com/seegno/sequelize-sql-tag/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/seegno/sequelize-sql-tag.git"
  },
  "main": "./dist/index.js",
  "keywords": [
    "es6",
    "multiline",
    "plugin",
    "sequelize",
    "sql",
    "sql-tag",
    "string",
    "strings",
    "tagged",
    "template"
  ],
  "options": {
    "mocha": "--compilers js:babel/register --require should test"
  },
  "scripts": {
    "build": "rm -rf dist/* && ./node_modules/.bin/babel src/ --loose --out-dir dist/",
    "changelog": "github_changelog_generator --no-issues",
    "cover": "./node_modules/.bin/babel-node node_modules/.bin/isparta cover --report html node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "lint": "eslint src test",
    "prepublish": "npm test && npm run build",
    "test": "./node_modules/.bin/mocha $npm_package_options_mocha"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.23",
    "eslint": "^0.24.1",
    "eslint-plugin-babel": "^1.1.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "mocha": "^2.2.5",
    "sequelize": "2.0.3",
    "should": "^7.0.3",
    "sqlite3": "^3.0.10"
  },
  "peerDependencies": {
    "sequelize": ">=1.7.0 <2.0.4"
  },
  "engines": {
    "node": ">= 0.10",
    "iojs": ">= 1.0.0"
  }
}
